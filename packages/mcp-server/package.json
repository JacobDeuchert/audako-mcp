{
  "name": "audako-ai",
  "version": "1.0.0",
  "description": "MCP Server for Audako",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "audako-ai": "dist/index.js"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc && npm run copy-docs",
    "copy-docs": "cp -r docs dist/",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "prepare": "npm run build",
    "test": "npx @modelcontextprotocol/inspector node dist/index.js",
    "update-core": "npm i https://github.com/JacobDeuchert/audako-core"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "audako"
  ],
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.0",
    "audako-core": "github:JacobDeuchert/audako-core",
    "dotenv": "^17.2.3",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
